import java.util.Collection;
import java.util.Set;
import java.util.HashSet;
import java.util.Iterator;

public class Hashsets implements Set {
    private HashSet<String> Web = new HashSet<String>();// se inician los hashtable de web
    private HashSet<String> Java = new HashSet<String>();// se inicia los hashtable de java
    private HashSet<String> Celular = new HashSet<String>();// se inica el hashtable de celular

    //sets y gets
    public HashSet<String> getWeb(){ return Web;}
    public HashSet<String> getJava(){return Java;}
    public HashSet<String> getCelular(){return Celular;}
    public void setJava(HashSet<String> java) {Java = java;}
    public void setCelular(HashSet<String> celular) {Celular = celular;}
    public void setweb(HashSet<String> web) {Web = web;}
    
   //guarda todo donde va
    public void contenido(String nombre, boolean java, boolean web, boolean celular){
        if (java==true){Java.add(nombre);}
        if(web==true){Web.add(nombre);}
        if (celular==true){Celular.add(nombre);}
        
    }

    @Override
    public int size() {
