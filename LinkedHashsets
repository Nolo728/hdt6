import java.util.Collection;
import java.util.Iterator;
import java.util.LinkedHashSet;
import java.util.Set;

public class LinkedHashsets implements Set{
  private  LinkedHashSet<String> Web = new LinkedHashSet<String>();// se inician los LinkedHashSet de web
    private LinkedHashSet<String> Java = new LinkedHashSet<String>();// se inicia los LinkedHashSet de java
    private LinkedHashSet<String> Celular = new LinkedHashSet<String>();// se inica el LinkedHashSet de celular
    
    //sets y gets
    public LinkedHashSet<String> getWeb(){ return Web;}
    public LinkedHashSet<String> getJava(){return Java;}
    public LinkedHashSet<String> getCelular(){return Celular;}
    public void setJava(LinkedHashSet<String> java) {Java = java;}
    public void setCelular(LinkedHashSet<String> celular) {Celular = celular;}
    public void setweb(LinkedHashSet<String> web) {Web = web;}
    
   //guarda todo donde va
    public void contenido(String nombre, boolean java, boolean web, boolean celular){
        if (java==true){Java.add(nombre);}
        if(web==true){Web.add(nombre);}
        if (celular==true){Celular.add(nombre);}
        
    }    

    @Override
    public int size() {
